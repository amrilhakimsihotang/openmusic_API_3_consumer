{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "no-underscore-dangle":"off",
        "camelcase":"off",
        "no-trailing-spaces":"off",
        "indent":"off",
        "no-multiple-empty-lines":"off",
        "eol-last":"off"
    }
}
